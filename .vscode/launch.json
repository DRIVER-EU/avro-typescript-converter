  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceFolder}\\dist\\src\\index.js",
      "args": [
        "-v",
        "example/system_topic_access_invite-value.avsc",
        "example/system_timing-value.avsc",
        "example/standard_cap-value.avsc",
        "missing_file_test",
        "-o",
        "output"
      ],
      "cwd": "${workspaceFolder}",
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ]
    }
  ]
}